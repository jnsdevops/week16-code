pipeline {
    agent any
    tools {
        maven "M2_HOME"
    }
    stages {
        stage ('sonarqube scan'){
            steps{
                withSonarQubeEnv ('sonar-server'){
                sh 'mvn verify org.sonarsource.scanner.maven:sonar-maven-plugin:3.10.0.2594:sonar -Dsonar.projectKey=jnsdevops_week16-code'
                }
            }
        }
        stage ('all maven command'){
            steps{
                sh 'mvn clean compile install package'
            }
        }
        
    }

}